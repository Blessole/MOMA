<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorator">
	<decorator name="decorator" page="view.jsp">
		<pattern>/views/board/*</pattern>
		<pattern>/views/content/*</pattern>
		<pattern>/views/member/*</pattern>
		<pattern>/views/myPage/*</pattern>
		<pattern>/views/searchResult.so</pattern>
	</decorator>
		<decorator name="decorator_admin" page="view_admin.jsp">
		<pattern>/views/admin/*</pattern>
	</decorator>
	<!-- 아래 프로그램은 예외이므로 decorator를 적용하지 마라 -->
	<excludes>
		<pattern></pattern>
	</excludes>
</decorators>